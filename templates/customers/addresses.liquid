<style>

  .tab {
    overflow: hidden;
    padding:0 0 0 34px;
    width:30%;
    float:left;  
  }
  .tabContent{
    float: right;
    width: 99%;
  }
  .tab button {
    transition: 0.3s;
    font-family: sofia-pro,sans-serif;
    font-size: 22px;
    letter-spacing: .02em;
    text-transform: uppercase;
    line-height: 1.2;
    margin: 0;

  }
  .tab .group-title--1::before {
    background-color: #fff;
  }
  .tab .group-title--1.active::before {
    background-color: #c5d9f3;
  }
  .tab .group-title--2::before {
    background-color: transparent;
  }
  .tab .group-title--2.active::before {
    background-color: #b8e7dc;
  }
  .tab .group-title--3::before {
    background-color: transparent;
  }
  .tab .group-title--3.active::before {
    background-color: #acacdd;
  }
  .tab .group-title--4::before {
    background-color: transparent;
  }
  .tab .group-title--4.active::before {
    background-color: #ffc8d3;
  }
  .tab .group-title--5::before {
    background-color: transparent;
  }
  .tab .group-title--5.active::before {
    background-color: #e0251b;
  }
  .tab .tit, th.tit {
    transition: 0.3s;
    font-family: sofia-pro,sans-serif;
    font-size: 22px;
    letter-spacing: .02em;
    text-transform: uppercase;
    line-height: 1.2;
    margin: 0;
  }
  th.tit {
    width:33.33%;
    border-bottom: 1px solid #000;
  }
  /* Style the tab content */
  .tabcontent {
    display: none;
    padding: 6px 12px;
    margin-top: 41px;
  }
  .nav__container {
    position:relative;
    padding: 60px 0 0 30px;
  }
  .tab .nav__container::last-child {
    padding-bottom:60px;
  }  
  .ContainerAccount{
    width:100%;
    overflow:hidden;
    padding-bottom: 60px;
    padding-right:30px;
  }
  .nav__container .group-title:before {
    position: absolute;
    content: '';
    width: 120px;
    height: 120px;
    /*top: calc(50% - 27px);
    left: calc(50% - 308px);*/
    top: 12px;
    left: -60px;
    background-color: transparent;
    border-radius: 100%;
    z-index: -1;
  }
  .bb-000{
    border-bottom:1px solid #000;
    padding-bottom:5px;
  }
  .Segment__Content p{
    font-size: 26px;
    margin: 10px 0 0 0;
  }
  .AccountTable .Heading td{
    font-family:'baskerville-display-pt';
    font-size:26px;
    padding-bottom:0;
    border:none;
  }
  .AccountTable .Heading td a{
    font-family:'baskerville-display-pt';
    font-size:26px;
    border:none;
    text-decoration:none!important;
  }
  .SectionHeader h1{
    color:#acacdc;
  }
  .linkLogout{
    font-family: 'baskerville-display-pt';
    font-size:25px;
  }
  .Email{
    margin-bottom: 40px;
  }
  .Email .titSection{
    font-family:'sofia-pro';
    font-size:22px;
    margin-bottom: 10px;
    text-align:left;
  }  
  .Email a.txtLink{
    font-family:'baskerville-display-pt';
    font-size:25px;
    margin-left:30px;
    display:inline-block;
  }  
  .Email a.txtLinkMobile{
    display:none;
  }  
  .Email p{
    font-family:'baskerville-display-pt';
    font-size:20px;
    line-height: 26px;
    width: 55%;
    text-align: left;
  }  
  .titS{
    display:none;
  }  
  .titS2 {
    margin-bottom: 40px;
    text-align: left;
    font-size:22px;
    color:#000;
  }
  .share{
    background: #c5d9f3;
    padding: 5px 0;
    cursor: pointer;
    text-align: center;
  }
  .share a{
    font-family: 'sofia-pro';
    font-size: 22px;
    line-height: 30px;
    color: #fff;
    font-weight: 300;
  }
  .titDetails{
    font-family:'sofia-pro';
    font-size:26px;
    line-height: 40px;
  }
  .subTitDetails{
    font-family:'baskerville-display-pt';
    font-size:26px;
    line-height: 40px;
  }
  .AccountAddress, .AccountAddress p,
  .AccountAddress span{
    font-family:'baskerville-display-pt';
    font-size:20px;
    line-height: 30px;
    padding-top: 5px;
  }
  .BBBlack{   
    border-bottom: 1px solid #000;
  }  
  .titSection2{
    font-family: 'sofia-pro';
    font-size: 22px;
    line-height: 30px;
  }
  .detailsSection2{
    font-family:'baskerville-display-pt';
    font-size:20px;
    line-height: 26px;
  }
  .TTUpper{
    text-transform:uppercase;
  }
  .detailsSection2 .AccountAddress, 
  .detailsSection2 .AccountAddress p, 
  .detailsSection2 .AccountAddress span {
    font-size: 26px;
    line-height: 30px;
  }
  a.linkLogout {
    margin-top: 14px;
    display: block;
  } 
  @media (max-width: 1100px){
    .nav__container {
      margin-bottom: 0;
    }
    .nav__container .group-title {
      margin-bottom: 0;
    }

    .tab {
      padding:26px 0 20px 12px;
      display: contents;
    }

    .nav__container .group-title:before {
      width: 80px;
      height: 80px;
      /*top: calc(50% - 22px);
      left: calc(50% - 207px);*/
      top: 13px;
      left: -25px;
    }
    .tab button {
      font-size: 25px;
    }
    .tabContent {
      width: 100%;
    }
    .AccountTable .Heading td, .AccountTable .Heading td a{
      font-size:17px;
    }
    th.tit {
      font-size: 12px;
    }
    .ContainerAccount {
      padding-right: 0;
    }
    .nav__container {
      padding: 36px 0 0 20px;
    }
    .TableWrapper>.Table {
      padding-right: 10px;
      padding-left: 10px;
    }
    .Email .titSection{
      text-align:center;
    }
    .Email a.txtLink{
      display:none
    }
    .Email a.txtLinkMobile{
      display:block;
    }
    .Email .titSection {
      font-size: 21px;
    }
    .Email p {
      font-size: 21px;
      line-height: 29px;
      width: 100%;
      text-align: center;
    }
    .Email a.txtLinkMobile{
      font-family:'sofia-pro';
      font-size:22px;
      padding:0;
      border:1px solid #000;
      text-transform:uppercase;
      margin-top: 30px;
    }
    #ACCOUNT1 .titSection, #ACCOUNT1 .Email p{
      text-align:left;
    }
    #ACCOUNT1 .titSection a.txtLink{
      display:inline-block;
    }
    .titS, .titS2 {
      margin-bottom: 40px;
      text-align: center;
      font-family:'sofia-pro';
      font-size:25px;
      line-height:70px;
      text-transform:uppercase;
      margin-bottom: 20px;
      font-weight: 400;
      color:#000;
      letter-spacing: 0.03em;
      display:block;
    }
    .share {
      margin-bottom: 70px;
    }
    .share a{
      line-height: 45px;
    }
    .AccountAddress, .AccountAddress p, .AccountAddress span {
      font-size: 16px;
      line-height: 17px;
    }
    .titSection2 {
      font-size: 16px;
    }
    .Button {
      font-size: 16px;
    }   
  } 
</style>
<style>
  .titAddress{
    font-family:'sofia-pro';
    font-size:26px;
    line-height: 40px;
  }
  .AccountAddress, .AccountAddress p,
  .AccountAddress span{
    font-family:'baskerville-display-pt';
    font-size:20px;
    line-height: 30px;
    padding-top: 5px;
  }
  .labelLogin {
    font-family: 'sofia-pro';
    font-size: 22px;
    text-transform: uppercase;
  }
  .tabcontent
  {
    margin-top:0px;
  }
  header.PageHeader.add-head {
    margin-top: 0px;
  }
</style>

<div class="ContainerAccount">
  <header class="PageHeader">
    <div class="SectionHeader SectionHeader--center">
      <h1>{{ 'customer.addresses.title' | t }}</h1>
    </div>
  </header>

  <div class="tab">
    <div class="nav__container">
      <button class="tablinks group-title group-title--1 active"  onclick="openCity(event, 'ACCOUNT1')">ADDRESS</button>
    </div>
    <div class="nav__container">
      <button class="tablinks group-title group-title--2" onclick="openCity(event, 'ACCOUNT2')">ORDER HISTORY</button>
    </div>
    <div class="nav__container">
      <button class="tablinks group-title group-title--3 " onclick="openCity(event, 'ACCOUNT3')">WISHLIST</button>
    </div>
    <div class="nav__container">
      <button class="tablinks group-title group-title--4" onclick="openCity(event, 'ACCOUNT4')">EMAILS PREFERENCES</button>
    </div>
    <!--    <div class="nav__container">
<button class="tablinks group-title group-title--5" onclick="openCity(event, 'ACCOUNT5')">&nbsp;</button>
</div> -->
    <div class="nav__container">
      <a href="{{ routes.account_logout_url }}" class="linkLogout">{{ 'customer.account.logout' | t }}</a>
    </div>
  </div>

  <!-- Tab content -->
  <div class="PageLayout PageLayout--breakLap"> 
    <div class="tabContent">
      <div id="ACCOUNT1" class="tabcontent" style="display: block;">
        <div class="Container Container--narrow" data-section-type="addresses" data-section-id="addresses">
          <header class="PageHeader add-head">
            {% comment %}
            <a href="{{ routes.account_url }}" class="PageHeader__Back Heading Text--subdued Link Link--primary u-h7">{% render 'icon' with 'select-arrow-left' %} {{ 'customer.account.back_to_account' | t }}</a>
            {% endcomment %}
            <div class="SectionHeader">
              {%- if customer.addresses_count == 0 -%}
              <p class="SectionHeader__Description">{{ 'customer.addresses.empty' | t }}</p>
              {%- endif -%}
              <div class="SectionHeader__ButtonWrapper">
                <button type="button" class="Button Button--primary" data-action="open-modal" aria-controls="modal-address-new">{{ 'customer.addresses.add_address' | t }}</button>
              </div>
            </div>
          </header>

          {%- if customer.addresses_count > 0 -%}
          {%- paginate customer.addresses by 9 -%}
          <div class="Grid Grid--l AddressList">
            {%- for address in customer.addresses -%}
            <div class="Grid__Cell 1/2--tablet 1/3--lap-and-up">
              <div class="Segment">
                {%- if address == customer.default_address -%}
                <h2 class="titAddress">{{ 'customer.addresses.default_address_label' | t }}</h2>
                {%- else -%}
                {%- assign position = paginate.current_page | times: forloop.index -%}
                <h2 class="titAddress">{{ 'customer.addresses.address_label' | t: position: position }}</h2>
                {%- endif -%}
                <div class="Segment__Content">
                  {{ address | format_address | replace: '<p>', '<p class="AccountAddress"><span>' | replace_first: '<br>', '</span><br>' }}
                  <div class="Segment__ActionList">
                    <button class="Segment__ActionItem Link Link--underline" data-action="open-modal" aria-controls="modal-{{ address.id }}">{{ 'customer.addresses.edit' | t }}</button>
                    <button class="Segment__ActionItem Link Link--underline" onclick="Shopify.CustomerAddress.destroy({{ address.id }}); return false">{{ 'customer.addresses.delete' | t }}</button>
                  </div>
                </div>
              </div>
            </div>
            {%- endfor -%}
          </div>
          {%- render 'pagination', paginate: paginate -%}
          {%- endpaginate -%}
          {%- endif -%}
        </div>

        {% comment %}FORM FOR NEW ADDRESS{% endcomment %}

        <div id="modal-address-new" class="Modal Modal--address" aria-hidden="true" role="dialog" data-scrollable>
          <button class="Modal__Close Modal__Close--outside" data-action="close-modal">{%- render 'icon' with 'close' -%}</button>
          <header class="Modal__Header">
            <h3 class="labelLogin">{{ 'customer.addresses.add_address' | t }}</h3>
            <p class="Modal__Description">{{ 'customer.addresses.form_subtitle' | t }}</p>
          </header>
          <div class="Modal__Content">
            {% form 'customer_address', customer.new_address, class: 'Form Form--spacingTight' %}
            {%- if request.locale.iso_code == 'ja' -%}
            <div class="Form__Item">
              <label class="labelLogin">{{ 'customer.addresses.last_name' | t }}</label>
              <input type="text" class="Form__Input" name="address[last_name]" value="{{ form.last_name }}" placeholder="{{ 'customer.addresses.last_name' | t }}" aria-label="{{ 'customer.addresses.last_name' | t }}">
            </div>
            <div class="Form__Item">
              <label class="labelLogin">{{ 'customer.addresses.first_name' | t }}</label>
              <input type="text" class="Form__Input" name="address[first_name]" value="{{ form.first_name }}" placeholder="{{ 'customer.addresses.first_name' | t }}" aria-label="{{ 'customer.addresses.first_name' | t }}" autofocus>
            </div>
            {%- else -%}
            <div class="Form__Item">
              <label class="labelLogin">{{ 'customer.addresses.first_name' | t }}</label>
              <input type="text" class="Form__Input" name="address[first_name]" value="{{ form.first_name }}" placeholder="{{ 'customer.addresses.first_name' | t }}" aria-label="{{ 'customer.addresses.first_name' | t }}" autofocus>
            </div>
            <div class="Form__Item">
              <label class="labelLogin">{{ 'customer.addresses.last_name' | t }}</label>
              <input type="text" class="Form__Input" name="address[last_name]" value="{{ form.last_name }}" placeholder="{{ 'customer.addresses.last_name' | t }}" aria-label="{{ 'customer.addresses.last_name' | t }}">
            </div>
            {%- endif -%}
            <div class="Form__Item">
              <label class="labelLogin">{{ 'customer.addresses.company' | t }}</label>
              <input type="text" class="Form__Input" name="address[company]" value="{{ form.company }}" placeholder="{{ 'customer.addresses.company' | t }}" aria-label="{{ 'customer.addresses.company' | t }}">
            </div>
            <div class="Form__Item">
              <label class="labelLogin">{{ 'customer.addresses.phone' | t }}</label>
              <input type="text" class="Form__Input" name="address[phone]" value="{{ form.phone }}" placeholder="{{ 'customer.addresses.phone' | t }}" aria-label="{{ 'customer.addresses.phone' | t }}">
            </div>
            <div class="Form__Item">
              <label class="labelLogin">{{ 'customer.addresses.address1' | t }}</label>
              <input type="text" class="Form__Input" name="address[address1]" value="{{ form.address1 }}" placeholder="{{ 'customer.addresses.address1' | t }}" aria-label="{{ 'customer.addresses.address1' | t }}">
            </div>
            <div class="Form__Item">
              <label class="labelLogin">{{ 'customer.addresses.address2' | t }}</label>
              <input type="text" class="Form__Input" name="address[address2]" value="{{ form.address2 }}" placeholder="{{ 'customer.addresses.address2' | t }}" aria-label="{{ 'customer.addresses.address2' | t }}">
            </div>
            <div class="Form__Item">
              <label class="labelLogin">{{ 'customer.addresses.city' | t }}</label>
              <input type="text" class="Form__Input" name="address[city]" value="{{ form.city }}" placeholder="{{ 'customer.addresses.city' | t }}" aria-label="{{ 'customer.addresses.city' | t }}">
            </div>
            <div class="Form__Group">
              <div class="Form__Item">
                <label class="labelLogin">COUNTRY</label>
                <div class="Form__Select Select Select--primary">
                  {%- render 'icon' with 'select-arrow' -%}
                  <select name="address[country]" title="{{ 'customer.addresses.country' | t }}">{{ all_country_option_tags }}</select>
                </div>
              </div>
              <div class="Form__Item">
                <label class="labelLogin">{{ 'customer.addresses.zip' | t }}</label>
                <input type="text" class="Form__Input" name="address[zip]" value="{{ form.zip }}" placeholder="{{ 'customer.addresses.zip' | t }}" aria-label="{{ 'customer.addresses.zip' | t }}">
              </div>
            </div>
            <div class="Form__Item Form__Select Select Select--primary" style="display: none">
              <label class="labelLogin">PROVINCE</label>
              {%- render 'icon' with 'select-arrow' -%}
              <select name="address[province]" title="{{ 'customer.addresses.province' | t }}"></select>
            </div>
            <div class="Form__Item">
              <div class="Form__CheckboxWrapper">
                <input type="checkbox" class="Form__Checkbox" name="address[default]" id="address-new[default]" value="0">
                {% render 'icon' with 'checkmark' %}
                <label for="address-new[default]">{{ 'customer.addresses.set_default' | t }}</label>
              </div>
            </div>
            <button class="Form__Submit Button Button--account Button--primary Button--full">{{ 'customer.addresses.add_address' | t }}</button>
            {% endform %}
          </div>
        </div>

        {% comment %}FORM FOR EXISTING ADDRESSES{% endcomment %}

        {%- paginate customer.addresses by 9 -%}
        {%- for address in customer.addresses -%}
        <div id="modal-{{ address.id }}" class="Modal Modal--address" aria-hidden="true" role="dialog" data-scrollable>
          <button class="Modal__Close Modal__Close--outside" data-action="close-modal">{%- render 'icon' with 'close' -%}</button>
          <header class="Modal__Header">
            <h3 class="labelLogin">{{ 'customer.addresses.edit_address' | t }}</h3>
            <p class="Modal__Description">{{ 'customer.addresses.form_subtitle' | t }}</p>
          </header>
          <div class="Modal__Content">
            {% form 'customer_address', address, class: 'Form Form--spacingTight' %}
            {%- if request.locale.iso_code == 'ja' -%}
            <div class="Form__Item">
              <label class="labelLogin">{{ 'customer.addresses.last_name' | t }}</label>
              <input type="text" class="Form__Input" name="address[last_name]" value="{{ form.last_name }}" placeholder="{{ 'customer.addresses.last_name' | t }}" aria-label="{{ 'customer.addresses.last_name' | t }}">
            </div>
            <div class="Form__Item">
              <label class="labelLogin">{{ 'customer.addresses.first_name' | t }}</label>
              <input type="text" class="Form__Input" name="address[first_name]" value="{{ form.first_name }}" placeholder="{{ 'customer.addresses.first_name' | t }}" aria-label="{{ 'customer.addresses.first_name' | t }}" autofocus>
            </div>
            {%- else -%}
            <div class="Form__Item">
              <label class="labelLogin">{{ 'customer.addresses.first_name' | t }}</label>
              <input type="text" class="Form__Input" name="address[first_name]" value="{{ form.first_name }}" placeholder="{{ 'customer.addresses.first_name' | t }}" aria-label="{{ 'customer.addresses.first_name' | t }}" autofocus>
            </div>
            <div class="Form__Item">
              <label class="labelLogin">{{ 'customer.addresses.last_name' | t }}</label>
              <input type="text" class="Form__Input" name="address[last_name]" value="{{ form.last_name }}" placeholder="{{ 'customer.addresses.last_name' | t }}" aria-label="{{ 'customer.addresses.last_name' | t }}">
            </div>
            {%- endif -%}
            <div class="Form__Item">
              <label class="labelLogin">{{ 'customer.addresses.company' | t }}</label>
              <input type="text" class="Form__Input" name="address[company]" value="{{ form.company }}" placeholder="{{ 'customer.addresses.company' | t }}" aria-label="{{ 'customer.addresses.company' | t }}">
            </div>
            <div class="Form__Item">
              <label class="labelLogin">{{ 'customer.addresses.phone' | t }}</label>
              <input type="text" class="Form__Input" name="address[phone]" value="{{ form.phone }}" placeholder="{{ 'customer.addresses.phone' | t }}" aria-label="{{ 'customer.addresses.phone' | t }}">
            </div>
            <div class="Form__Item">
              <label class="labelLogin">{{ 'customer.addresses.address1' | t }}</label>
              <input type="text" class="Form__Input" name="address[address1]" value="{{ form.address1 }}" placeholder="{{ 'customer.addresses.address1' | t }}" aria-label="{{ 'customer.addresses.address1' | t }}">
            </div>
            <div class="Form__Item">
              <label class="labelLogin">{{ 'customer.addresses.address2' | t }}</label>
              <input type="text" class="Form__Input" name="address[address2]" value="{{ form.address2 }}" placeholder="{{ 'customer.addresses.address2' | t }}" aria-label="{{ 'customer.addresses.address2' | t }}">
            </div>
            <div class="Form__Item">
              <label class="labelLogin">{{ 'customer.addresses.city' | t }}</label>
              <input type="text" class="Form__Input" name="address[city]" value="{{ form.city }}" placeholder="{{ 'customer.addresses.city' | t }}" aria-label="{{ 'customer.addresses.city' | t }}">
            </div>
            <div class="Form__Group">
              <div class="Form__Item">
                <label class="labelLogin">COUNTRY</label>
                <div class="Form__Select Select Select--primary">
                  {%- render 'icon' with 'select-arrow' -%}
                  <select name="address[country]" title="{{ 'customer.addresses.country' | t }}" data-default="{{ form.country }}">{{ all_country_option_tags }}</select>
                </div>
              </div>
              <div class="Form__Item">
                <label class="labelLogin">{{ 'customer.addresses.zip' | t }}</label>
                <input type="text" class="Form__Input" name="address[zip]" value="{{ form.zip }}" placeholder="{{ 'customer.addresses.zip' | t }}" aria-label="{{ 'customer.addresses.zip' | t }}">
              </div>
            </div>
            <div class="Form__Item Form__Select Select Select--primary" style="display: none">
              <label class="labelLogin">PROVINCE</label>
              {%- render 'icon' with 'select-arrow' -%}
              <select name="address[province]" title="{{ 'customer.addresses.province' | t }}" data-default="{{ form.province }}"></select>
            </div>
            <div class="Form__Item">
              <div class="Form__CheckboxWrapper">
                <input type="checkbox" class="Form__Checkbox" id="address-{{ address.id }}[default]" name="address[default]" {% if address.id == customer.default_address.id %}value="1" checked{% endif %}>
                {% render 'icon' with 'checkmark' %}   <label for="address-{{ address.id }}[default]">{{ 'customer.addresses.set_default' | t }}</label>
              </div>
            </div>
            <button class="Form__Submit Button Button--primary Button--full">{{ 'customer.addresses.edit_address' | t }}</button>
            {% endform %}
          </div>
        </div>
        {%- endfor -%}
        {%- endpaginate -%}

        {%- comment -%}
        --------------------------------------------------------------------------------------------------------------------
        ADDRESS
        --------------------------------------------------------------------------------------------------------------------

        <div class="titS2 TTUpper ">{{ 'customer.addresses.title' | t }}</div>
        <div class="PageLayout__Section PageLayout__Section--secondary ">
          <div class="Segment">
            {%- if customer.addresses_count == 0 -%}
            <h2 class="titSection2 BBBlack">{{ 'customer.account.no_addresses_title' | t }}</h2>

            <div class="detailsSection2">
              <p>{{ 'customer.account.no_addresses_content' | t }}</p>

              <div class="Segment__ButtonWrapper">
                <a href="{{ routes.account_addresses_url }}" class="Button Button--primary">{{ 'customer.account.manage_addresses' | t }}</a>
              </div>
            </div>
            {%- else -%}
            <h2 class="titSection2 BBBlack">{{ 'customer.account.default_address' | t }}</h2>

            <div class="detailsSection2">
              {{ customer.default_address | format_address | replace: '<p>', '<p class="AccountAddress"><span>' | replace_first: '<br>', '</span><br>' }}
              <div class="Segment__ButtonWrapper">

                <a href="javascript:void(0)" class="Button Button--account Button--primary" onclick="openCity(event, 'ACCOUNT6')">{{ 'customer.account.edit_addresses' | t }}</a>
              </div>
            </div>
            {%- endif -%}
          </div>
        </div>
        {%- endcomment -%}

      </div>
      <div id="ACCOUNT2" class="tabcontent">
        {%- comment -%}
        --------------------------------------------------------------------------------------------------------------------
        ORDER HISTORY
        --------------------------------------------------------------------------------------------------------------------
        {%- endcomment -%}
        <div class="titS">ORDER HISTORY</div>
        <div class="PageLayout__Section">
